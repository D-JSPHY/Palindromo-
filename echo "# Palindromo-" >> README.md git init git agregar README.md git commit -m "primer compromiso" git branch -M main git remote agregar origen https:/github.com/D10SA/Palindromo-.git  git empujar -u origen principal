var palindrome = function (x){
    let original = [];
    let lanigiro = [];
    for(let pal1 = 0; pal1 < x.length; pal1++){
        original.push(x[pal1]);
    };
    for(let pal2 = 0; pal2 < x.length; pal2++){
        lanigiro.push(x[pal2]);
    };
    var vul1 = original.join("");
    var vul2 = lanigiro.join("");
    if(vul1 == vul2){
        return true;
    }else{
        return false;
    }
}
